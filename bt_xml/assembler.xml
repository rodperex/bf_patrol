            </Sequence>
        </RetryUntilSuccessful>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="Assemble"/>
        <Action ID="GetComponents"/>
        <Action ID="Move"/>
        <Condition ID="TargetAccomplished"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>
